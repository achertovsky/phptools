{
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
        ]
    },
    "timeout": 10,
    "threads": "max",
    "logs": {
        "text": "var/infection/infection.log",
        "html": "var/infection/infection.html",
        "summary": "var/infection/summary.log",
        "json": "var/infection/infection-log.json",
        "perMutator": "var/infection/per-mutator.md",
        "github": true,
        "gitlab": "var/infection/gitlab-code-quality.json",
        "stryker": {
            "badge": "/^release-.*$/"
        },
        "summaryJson": "var/infection/summary.json"
    },
    "tmpDir": "var/infection/tmp-folder",
    "mutators": {
        "global-ignore": [
            "FooClass::__construct"
        ],
        "global-ignoreSourceCodeByRegex": [
            "Assert::.*"
        ],
        "@default": true,
        "@function_signature": false
    },
    "testFramework":"phpunit",
    "testFrameworkOptions": "",
    "staticAnalysisTool":"phpstan",
    "bootstrap":"./infection-bootstrap.php",
    "initialTestsPhpOptions": "-d zend_extension=xdebug.so"
}
